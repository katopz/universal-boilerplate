{
  "name": "universal-boilerplate",
  "version": "2.0.0",
  "description": "Isomorphic/universal boilerplate with react, redux, webpack and express",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint ./src",
    "serve:prod": "cross-env NODE_ENV=production babel-node ./server.js",
    "prod": "npm run build:prod && npm run serve:prod",
    "dev": "cross-env NODE_ENV=development babel-node ./server.js",
    "size": "cross-env NODE_ENV=production && rimraf dist && echo \"$(webpack --config ./webpack.config.prod.js --json | webpack-bundle-size-analyzer)\" > webpack.size.txt",
    "build:prod": "cross-env NODE_ENV=production && rimraf dist && webpack --config ./webpack.config.prod.js",
    "build": "npm run build:prod > webpack.stats.txt",
    "start": "react-scripts start",
    "build:static": "react-scripts build"
  },
  "author": "Justin Jung <justinjung04@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-3": "^6.17.0",
    "copy-webpack-plugin": "^4.0.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "eslint": "^3.9.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^6.6.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.11.2",
    "postcss-loader": "^1.1.1",
    "react-scripts": "^0.7.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "redux-logger": "^2.7.4",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/justinjung04/universal-boilerplate.git"
  },
  "dependencies": {
    "d3": "^4.3.0",
    "d3-array": "^1.0.1",
    "d3-axis": "^1.0.3",
    "d3-scale": "^1.0.3",
    "d3-selection": "^1.0.2",
    "d3-svg": "^0.2.0",
    "humps": "^2.0.0",
    "muicss": "^0.9.4",
    "normalizr": "^2.2.1",
    "preact": "^6.4.0",
    "preact-compat": "^3.9.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-lite": "^0.15.26",
    "react-mdl": "^1.7.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  }
}
