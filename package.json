{
  "name": "universal-boilerplate",
  "version": "2.0.0",
  "description": "Isomorphic/universal boilerplate with react, redux, webpack and express",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint ./src",
    "start:prod": "cross-env NODE_ENV=production babel-node ./server.js",
    "start:dev": "cross-env NODE_ENV=development babel-node ./server.js",
    "build:size": "cross-env NODE_ENV=production && rimraf dist && echo \"$(webpack --config ./webpack.config.prod.js --json | webpack-bundle-size-analyzer)\" > webpack.size.txt",
    "build:prod": "cross-env NODE_ENV=production && rimraf dist && webpack --config ./webpack.config.prod.js",
    "start": "npm run start:prod",
    "dev": "npm run start:dev",
    "build": "npm run build:prod > webpack.stats.txt",
    "serve": "cross-env NODE_ENV=production babel-node ./server.js"
  },
  "author": "Justin Jung <justinjung04@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015-minimal": "^2.1.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^6.2.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.9.3",
    "postcss-loader": "^0.13.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.7.0",
    "webpack-hot-middleware": "^2.12.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/justinjung04/universal-boilerplate.git"
  },
  "dependencies": {
    "d3": "^4.2.2",
    "d3-array": "^1.0.1",
    "d3-axis": "^1.0.3",
    "d3-scale": "^1.0.3",
    "d3-selection": "^1.0.2",
    "d3-svg": "^0.2.0",
    "muicss": "^0.7.5",
    "preact": "^6.0.2",
    "preact-compat": "^3.0.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-lite": "^0.15.18",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.6.0",
    "humps": "^1.1.0",
    "normalizr": "^2.2.1",
    "redux-thunk": "^2.1.0"
  }
}